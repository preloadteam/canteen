{
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "align": [ 1, "parameters", "statements", "members", "elements" ],
        "array-bracket-spacing": 1,
        "array-type": [ 1, "array-simple" ],
        "arrow-parens": 1,
        "arrow-return-shorthand": 1,
        "await-promise": 1,
        "ban": 0,
        "ban-comma-operator": 1,
        "class-name": 1,
        "comment-format": [ 1, "check-space" ],
        "curly": 1,
        "eofline": 1,
        "forin": 1,
        "interface-name": [1, "never-prefix"],
        "jsdoc-format": 1,
        "jsx-equals-spacing": [ 1, "newer" ],
        "jsx-no-lambda": 0,
        "jsx-no-multiline-js": 0,
        "jsx-no-string-ref": 1,
        "jsx-self-close": 1,
        "jsx-space-before-trailing-slash": 1,
        "label-position": 1,
        "import-spacing": 1,
        "max-classes-per-file": [ 1, 1 ],
        "max-line-length": [ 1, 130 ],
        "member-access": 1,
        "member-ordering": 0,
        "new-parens": 1,
        "newline-before-return": 1,
        "no-any": 1,
        "no-arg": 1,
        "no-bitwise": 0,
        "no-consecutive-blank-lines": [ 1, 1 ],
        "no-console": 1,
        "no-construct": 1,
        "no-debugger": 1,
        "no-duplicate-imports": 1,
        "no-duplicate-super": 1,
        "no-duplicate-switch-case": 1,
        "no-duplicate-variable": 1,
        "no-empty-interface": 0,
        "no-eval": 1,
        "no-floating-promises": 1,
        "no-for-in-array": 1,
        "no-implicit-dependencies": 0,
        "no-invalid-template-strings": 1,
        "no-irregular-whitespace": 1,
        "no-parameter-reassignment": 1,
        "no-redundant-jsdoc": 0,
        "no-return-await": 1,
        "no-shadowed-variable": 1,
        "no-sparse-arrays": 1,
        "no-string-literal": 1,
        "no-string-throw": 1,
        "no-submodule-imports": 0,
        "no-switch-case-fall-through": 1,
        "no-this-assignment": [ 1, { "allow-destructuring": 1 } ],
        "no-trailing-whitespace": 1,
        "no-unnecessary-class": [ 1, "allow-static-only" ],
        "no-unsafe-finally": 1,
        "no-unused-expression": 1,
        "no-use-before-declare": 1,
        "no-var-keyword": 1,
        "no-var-requires": 0,
        "object-curly-spacing": 1,
        "object-literal-sort-keys": 0,
        "one-line": [ 1, "check-catch", "check-else", "check-open-brace", "check-whitespace" ],
        "ordered-imports": 0,
        "prefer-for-of": 0,
        "prefer-conditional-expression": 0,
        "prefer-const": 1,
        "prefer-method-signature": 1,
        "quotemark": [ 1, "single", "jsx-double" ],
        "radix": 1,
        "restrict-plus-operands": 1,
        "semicolon": [ 1, "always" ],
        "switch-default": 1,
        "switch-final-break": [ 1, "always" ],
        "ter-indent": [ 1, 4 ],
        "trailing-comma": [ 1, { "multiline": "never", "singleline": "never" } ],
        "triple-equals": [ 1, "allow-null-check" ],
        "typedef": [ 1, "property-declaration" ],
        "typedef-whitespace": [
            1,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "use-isnan": 1,
        "variable-name": [ 1, "ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case" ],
        "whitespace": [
            1,
            "check-branch",
            "check-decl",
            "check-module",
            "check-operator",
            "check-separator",
            "check-type",
            "check-typecast",
            "check-type-operator"
        ]
    },
    "parserOptions": {
        "sourceType": "module"
    }
}
